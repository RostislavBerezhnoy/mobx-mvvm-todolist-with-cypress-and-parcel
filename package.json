{
  "name": "mobx-mvvm-todolist-with-cypress-and-parcel",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/RostislavBerezhnoy/mobx-mvvm-todolist-with-cypress-and-parcel.git",
  "author": "Rostislav Berezhnoy <berezhnoy.rostislav@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npx parcel public/index.html --port 3000 --open",
    "start-server": "npx json-server --watch db.json --host 127.0.0.1 --port 5000",
    "lint": "tsc --noEmit && eslint --fix \"**/*.@(js|jsx|ts|tsx)\"",
    "cypress:open": "cypress open",
    "prebuild": "yarn run clean-linux && yarn run clean-windows",
    "build": "parcel build public/index.html --no-source-maps",
    "clean-linux": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf ./dist",
    "clean-windows": "node -e \"if (process.platform === 'win32') process.exit(1)\" || rmdir /s /q dist"
  },
  "alias": {
    "components": "./src/components",
    "config": "./src/config",
    "mvvm": "./src/mvvm",
    "pages": "./src/pages",
    "store": "./src/store",
    "types": "./src/types"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@types/node": "20.0.0",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cypress": "^12.13.0",
    "dotenv": "^16.1.4",
    "eslint": "~8.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "parcel-bundler": "^1.12.5",
    "postcss": "^7",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "~5.0.4"
  }
}
